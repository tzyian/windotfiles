{
  "telemetry.telemetryLevel": "off",
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "editor.inlineSuggest.enabled": true,
  "code-runner.enableAppInsights": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "jsonc": false,
    "haskell": true
  },
  "haskell.manageHLS": "GHCup",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.tabSize": 2,
  "rust-analyzer.cargo.buildScripts.overrideCommand": null,
  "editor.formatOnSave": true,
  "notebook.lineNumbers": "on",
  "breadcrumbs.enabled": true,
  "notebook.formatOnSave.enabled": true,
  "rust-analyzer.check.overrideCommand": null,
  "remote.SSH.remotePlatform": {
    "pe114": "linux",
    "xlog.comp.nus.edu.sg": "linux",
    "soctf-pdc-009.d1.comp.nus.edu.sg": "linux",
    "pdc10": "linux",
    "pdc3": "linux",
    "pdc1": "linux",
    "pdc9": "linux",
    "pdc11": "linux",
    "pdc": "linux",
    "xlog": "linux"
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "zig": "zig run",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "$pythonPath $fullFileName",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runghc",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName",
    "mojo": "mojo run"
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1,
    "vscodevim.vim": 1
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "editor.linkedEditing": true,
  "editor.guides.bracketPairs": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "gitlens.telemetry.enabled": false,
  "C_Cpp.autocompleteAddParentheses": true,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "window.openFilesInNewWindow": "default",
  "window.openFoldersInNewWindow": "on",
  "cSpell.userWords": [
    "CUDA",
    "GPGPUs",
    "MIMD",
    "MISD",
    "Multicore",
    "Parallelisation",
    "SIMD",
    "SPMD",
    "Synchronisation"
  ],
  "cSpell.language": "en,en-GB",
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "note",
      "color": "#D36E70",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.colorCustomizations": {
    "statusBar.background": "#023b1a",
    "statusBar.noFolderBackground": "#023b1a",
    "statusBar.debuggingBackground": "#023b1a",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.debuggingForeground": "#FFFFFF"
  },
  /************ VSCODE NVIM **************/
  "vscode-neovim.useWSL": true,
  "vscode-neovim.neovimExecutablePaths.linux": "/opt/nvim-linux-x86_64/bin/nvim",
  "nvim-ui.nvimColorNormal": "#023b1a",
  "nvim-ui.nvimColorInsert": "#023557",
  "nvim-ui.nvimColorVisual": "#460157",
  "nvim-ui.nvimColorReplace": "#000",
  "nvim-ui.nvimColorCustomizationKeys": [
    "statusBar.background"
  ],
  /****** WHICH-KEY SETTINGS *******/
  "whichkey.delay": 0,
  "whichkey.bindings": [
    {
      "key": "w",
      "name": "Save",
      "type": "command",
      "command": "workbench.action.files.save"
    },
    {
      "key": "n",
      "name": "Toggle explorer",
      "when": "editorTextFocus",
      "type": "command",
      "command": "workbench.view.explorer"
    },
    {
      "key": "j",
      "name": "SplitJoin",
      "type": "command",
      "when": "editorTextFocus && neovim.mode != insert",
      "command": "vscode-neovim.send",
      "args": "<space>j"
    },
    /*{
      "key": "<left>",
      "name": "Move parameter left",
      "type": "command",
      "command": "vscode-neovim.send",
      "args": "<space><left>"
    },
    {
      "key": "<right>",
      "name": "Move parameter right",
      "type": "command",
      "command": "vscode-neovim.send",
      "args": "<space><right>"
    }, */
    {
      "key": "?",
      "name": "Search keybindings",
      "type": "command",
      "command": "whichkey.searchBindings"
    },
    {
      "key": "z",
      "name": "+Folds",
      "type": "bindings",
