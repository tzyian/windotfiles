    {
      "key": "e",
      "name": "Hover",
      "type": "command",
      "command": "editor.action.showHover"
    },
    {
      "key": "f",
      "name": "+Find",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "File",
          "type": "command",
          "command": "workbench.action.quickOpen"
        },
        {
          "key": "g",
          "name": "Grep",
